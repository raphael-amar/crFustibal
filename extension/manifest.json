{
	"name": "Rongo Rongo Extension",
	"version": "1.1",
	"background": {
		"persistent": false,
		"scripts": ["background.js"]
	},
	"options_page": "options.html",
	"permissions": ["tabs", "http://*/*"],
	"browser_action": {
		"name": "Rongo Rongo Extension"
	},
	"content_scripts": [
		{
			"matches": ["http://*/*", "https://*/*"],
			"js": ["content.js"],
			"run_at": "document_end"
		}
	]
	,
	"manifest_version": 2
}